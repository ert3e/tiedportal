-- --------------------------------------------------------

--
-- Table structure for table `orders`
--

CREATE TABLE `orders` (
  `orderID` int(10) UNSIGNED NOT NULL,
  `schoolID` int(10) UNSIGNED NOT NULL,
  `instructorID` int(10) UNSIGNED NOT NULL,
  `customerID` int(10) UNSIGNED NOT NULL,
  `discoutID` int(10) UNSIGNED NOT NULL,
  `packageName` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `duration` int(11) NOT NULL,
  `validity` int(11) NOT NULL,
  `orderFrom` enum('oppima','widget') COLLATE utf8_unicode_ci NOT NULL,
  `discountType` enum('package','global','none') COLLATE utf8_unicode_ci NOT NULL,
  `discountvalue` double(8,2) NOT NULL,
  `price` double(8,2) NOT NULL,
  `finalPrice` double(8,2) NOT NULL,
  `paymentType` enum('cash','stripe','paypal') COLLATE utf8_unicode_ci NOT NULL,
  `paymentStatus` enum('paid','unpaid') COLLATE utf8_unicode_ci NOT NULL DEFAULT 'unpaid',
  `created_at` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `updated_at` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00'
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

