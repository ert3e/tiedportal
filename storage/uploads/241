-- --------------------------------------------------------

--
-- Table structure for table `modules`
--

CREATE TABLE `modules` (
  `moduleID` int(10) UNSIGNED NOT NULL,
  `name` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `description` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `createdon` datetime NOT NULL,
  `updatedon` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `status` enum('0','1') COLLATE utf8_unicode_ci NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

-- --------------------------------------------------------

--
-- Table structure for table `module_permission`
--

CREATE TABLE `module_permission` (
  `permissionID` int(10) UNSIGNED NOT NULL,
  `moduleID` int(10) UNSIGNED NOT NULL,
  `roleID` int(10) UNSIGNED NOT NULL,
  `cancreate` enum('0','1') COLLATE utf8_unicode_ci NOT NULL,
  `canread` enum('0','1') COLLATE utf8_unicode_ci NOT NULL,
  `canupdate` enum('0','1') COLLATE utf8_unicode_ci NOT NULL,
  `candelete` enum('0','1') COLLATE utf8_unicode_ci NOT NULL,
  `createdon` datetime NOT NULL,
  `updatedon` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `status` enum('0','1') COLLATE utf8_unicode_ci NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

-- --------------------------------------------------------

--
-- Table structure for table `notifications`
--

CREATE TABLE `notifications` (
  `id` int(10) UNSIGNED NOT NULL,
