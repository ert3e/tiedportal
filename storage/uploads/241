--
-- Table structure for table `settings`
--

CREATE TABLE `settings` (
  `homepagetitle` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `homepagemetadescription` varchar(200) COLLATE utf8_unicode_ci NOT NULL,
  `taxdiscountin` enum('fixed','percent') COLLATE utf8_unicode_ci NOT NULL,
  `taxdiscountvalue` double(8,2) NOT NULL,
  `paypalTransactionFeePercentage` double(8,2) NOT NULL,
  `paypalTransactionFeeFixed` double(8,2) NOT NULL,
  `stripeTransactionFeePercentage` double(8,2) NOT NULL,
  `stripeTransactionFeeFixed` double(8,2) NOT NULL,
  `websiteCommissionIn` enum('fixed','percent') COLLATE utf8_unicode_ci NOT NULL,
  `websiteCommissionValue` double(8,2) NOT NULL,
  `widgetCommissionIn` enum('fixed','percent') COLLATE utf8_unicode_ci NOT NULL,
  `widgetCommissionValue` double(8,2) NOT NULL,
  `updated_by` int(10) UNSIGNED NOT NULL,
  `updated_at` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00'
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

--
-- Dumping data for table `settings`
--

