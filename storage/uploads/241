CREATE TABLE `school` (
  `schoolID` int(10) UNSIGNED NOT NULL,
  `name` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `company` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `logo` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `phone` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `mobile` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `fax` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `email` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `website` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `description` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `billingcompanyname` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `companynumber` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `vat` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `address` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `city` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `country` varchar(50) COLLATE utf8_unicode_ci NOT NULL,
  `postcode` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `contactname` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `contactemail` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `contactphone` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `acname` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `acnumber` bigint(20) NOT NULL,
  `bankcode` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `bankname` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `updatedon` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `status` enum('0','1') COLLATE utf8_unicode_ci NOT NULL,
  `accept_cash` enum('0','1') COLLATE utf8_unicode_ci NOT NULL DEFAULT '1',
  `accept_paypal` enum('0','1') COLLATE utf8_unicode_ci NOT NULL DEFAULT '1',
  `accept_stripe` enum('0','1') COLLATE utf8_unicode_ci NOT NULL DEFAULT '1'
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

